# Required API Keys
ANTHROPIC_API_KEY=your_claude_api_key
GOOGLE_API_KEY=your_gemini_api_key

# Optional Configuration
MULTI_SWARM_LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR
MULTI_SWARM_CACHE_DIR=.cache  # Directory for storing cached data
MULTI_SWARM_MAX_TOKENS=4096  # Maximum tokens per request
MULTI_SWARM_TEMPERATURE=0.7  # Default temperature for LLM requests

# RAG Configuration
MULTI_SWARM_EMBEDDING_MODEL=all-MiniLM-L6-v2
MULTI_SWARM_CHUNK_SIZE=1000
MULTI_SWARM_INDEX_PATH=.index

# Code Interpreter Configuration
MULTI_SWARM_INTERPRETER_TIMEOUT=30
MULTI_SWARM_INTERPRETER_MEMORY=1G

# File Storage Configuration
MULTI_SWARM_STORAGE_PATH=.storage
MULTI_SWARM_MAX_STORAGE=5G 