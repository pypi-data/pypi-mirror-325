        "watch": "npm run build:js::dev -- --watch",
        "dist": "python -m build --sdist --wheel --outdir dist/"
    },
    "devDependencies": {
        "@braintree/sanitize-url": "^7.1.0",
        "@types/ramda": "^0.30.1",
        "@types/react": "^18.2.55",
        "css-loader": "^6.10.0",
        "dash-extensions-js": "^0.0.8",
        "npm-run-all": "^2.1.0",
        "ramda": "^0.30.1",
        "react": "^18.2.0",
        "react-docgen": "^5.4.0",
        "react-dom": "^18.2.0",
        "style-loader": "^3.3.4",
        "ts-loader": "^9.5.1",
        "typescript": "5.6.3",
        "webpack": "^5.96.1",
        "webpack-cli": "^5.1.4"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "author": "Snehil Vijay <snehilvj@outlook.com>",
