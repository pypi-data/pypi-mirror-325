        <Param val="grids = [grid1_2, grid2_2, grid3_2, grid4_2, grid5_2, grid6_2, grid7_2, grid8_2, grid9_2];&amp;#10;resp_2.pos = [(- 0.5), (- 0.25)];&amp;#10;grid_dropped = &quot;&quot;;&amp;#10;showSubmit = false;&amp;#10;correct = 0;&amp;#10;time_taken = 0;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine" />
        <Param val="# create a list of grids to check where the square has been dropped&amp;#10;grids = [grid1_2, grid2_2, grid3_2, grid4_2, grid5_2, grid6_2, grid7_2, grid8_2, grid9_2]&amp;#10;&amp;#10;# reset the response square at the beginning of each trial&amp;#10;resp_2.pos = [-0.5, -0.25]&amp;#10;&amp;#10;# variable to save which grid the square was dropped in&amp;#10;grid_dropped = &quot;&quot;&amp;#10;&amp;#10;# only show the submit button when a square has been dropped&amp;#10;showSubmit = False&amp;#10;&amp;#10;# variable to store if the trial was correct&amp;#10;correct = 0&amp;#10;&amp;#10;# variable to store time taken to complete trial&amp;#10;time_taken = 0" valType="extendedCode" updates="constant" name="Begin Routine" />
        <Param val="Auto-&gt;JS" valType="str" updates="None" name="Code Type" />
