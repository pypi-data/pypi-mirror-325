        "install": "scoop bucket add extras ; scoop install vscode"
    },
    "draw.io": {
        "official_name": "diagrams.net (formerly draw.io)",
        "website": "https://www.diagrams.net/",
        "install": "scoop bucket add extras ; scoop install draw.io"
