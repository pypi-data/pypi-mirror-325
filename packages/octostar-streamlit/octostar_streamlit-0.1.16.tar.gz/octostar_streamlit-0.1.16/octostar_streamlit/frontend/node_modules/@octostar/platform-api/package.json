{
  "name": "@octostar/platform-api",
  "version": "0.5.3",
  "description": "Provides access to the API exposed by Octostar platform to an application run in an iframe",
  "keywords": [
    "octostar",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "",
    "directory": "superset-frontend/packages/octostar-platform-api"
  },
  "license": "MIT",
  "author": "",
  "main": "lib/index.js",
  "types": "dist/platform-api.d.ts",
  "files": [
    "dist/platform-api.d.ts",
    "lib/**/*"
  ],
  "scripts": {
    "browserify": "browserify lib/index.js -s octostar -t [ babelify --presets [@babel/preset-env] ]  -o dist/octostar-platform-api.js",
    "build": "npm run clear && npm run compile && npm run run:api-extractor && npm run browserify",
    "clear": "rimraf ./lib ./dist ./temp tsconfig.tsbuildinfo",
    "compile": "tsc",
    "dev": "tsc -b -w",
    "run:api-extractor": "api-extractor run --local --verbose"
  },
  "dependencies": {
    "post-robot": "^10.0.46"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.42.3",
    "@octostar/platform-types": "file:./packages/octostar-platform-types",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "@octostar/platform-types": "^0.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "9e4b6e63a51d720054e4d00b57481dcb276eaa19"
}
