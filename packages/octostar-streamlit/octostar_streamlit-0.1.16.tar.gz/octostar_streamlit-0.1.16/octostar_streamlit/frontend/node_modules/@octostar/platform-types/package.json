    "csstype": "^3.1.3",
    "flexlayout-react": "github:Octostarco/flexlayout-react#multi-window-drag-drop",
    "rc-menu": "^9.8.2",
    "rimraf": "^5.0.5",
    "typedoc": "^0.26.5"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
