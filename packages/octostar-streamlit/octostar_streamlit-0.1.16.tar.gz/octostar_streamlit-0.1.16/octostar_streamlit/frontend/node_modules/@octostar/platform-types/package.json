    "octostar",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "",
    "directory": "superset-frontend/packages/octostar-platform-types"
  },
  "license": "MIT",
  "author": "",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./dist/platform-types.d.ts"
    }
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
