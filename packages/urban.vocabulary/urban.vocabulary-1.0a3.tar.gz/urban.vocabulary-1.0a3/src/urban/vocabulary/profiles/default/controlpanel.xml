<?xml version="1.0"?>
<object
  name="portal_controlpanel"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="urban.vocabulary"
  purge="False">

  <configlet
    title="Urban Vocabulary"
    action_id="skipauthentication"
    appId="urban.vocabulary"
    category="Products"
    condition_expr=""
    url_expr="string:${portal_url}/@@urbanvocabulary-controlpanel"
    icon_expr="string:$portal_url/group.png"
    visible="True"
    i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>

</object>
