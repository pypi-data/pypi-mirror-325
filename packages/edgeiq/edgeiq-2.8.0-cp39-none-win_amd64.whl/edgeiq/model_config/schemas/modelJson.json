{
    "$id": "https://alwaysai.co/modelJson.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "unevaluatedProperties": false,
    "properties": {
        "accuracy": {
            "type": [
                "string",
                "null"
            ]
        },
        "dataset": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "id": {
            "type": "string",
            "pattern": "^[a-zA-Z0-9]+/[a-zA-Z0-9_-]+$"
        },
        "inference_time": {
            "type": [
                "number",
                "null"
            ]
        },
        "license": {
            "type": "string"
        },
        "mean_average_precision_top_1": {
            "type": [
                "number",
                "null"
            ]
        },
        "mean_average_precision_top_5": {
            "type": [
                "number",
                "null"
            ]
        },
        "model_parameters": {
            "$ref": "https://alwaysai.co/modelParameters.json#/properties/model_parameters"
        },
        "public": {
            "type": "boolean"
        },
        "website_url": {
            "type": "string"
        },
        "version": {
            "type": "number"
        }
    },
    "required": [
        "accuracy",
        "dataset",
        "description",
        "id",
        "license",
        "model_parameters",
        "public",
        "website_url"
    ]
}