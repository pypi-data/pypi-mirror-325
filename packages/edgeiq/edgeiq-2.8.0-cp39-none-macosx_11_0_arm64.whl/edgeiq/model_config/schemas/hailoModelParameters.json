{
    "$id": "https://alwaysai.co/hailoModelParameters.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "properties": {
        "hailoModelProperties": {
            "type": "object",
            "required": [
                "framework_type",
                "architecture",
                "quantize_input",
                "quantize_output",
                "input_format",
                "output_format"
            ],
            "properties": {
                "architecture": {
                    "type": "string",
                    "$ref": "HailoArchitecture"
                },
                "framework_type": {
                    "enum": [
                        "hailo"
                    ],
                    "type": "string"
                },
                "input_format": {
                    "type": "string",
                    "$ref": "HailoFormat"
                },
                "output_format": {
                    "type": "string",
                    "$ref": "HailoFormat"
                },
                "purpose": {
                    "enum": [
                        "ObjectDetection"
                    ],
                    "type": "string"
                },
                "quantize_input": {
                    "type": "boolean"
                },
                "quantize_output": {
                    "type": "boolean"
                }
            }
        },
        "HailoArchitecture": {
            "$id": "HailoArchitecture",
            "type": "string",
            "enum": [
                "mobilenet_ssd",
                "yolov3"
            ]
        },
        "HailoFormat": {
            "$id": "HailoFormat",
            "type": "string",
            "enum": [
                "auto",
                "float32",
                "none",
                "uint16",
                "uint8"
            ]
        }
    }

}